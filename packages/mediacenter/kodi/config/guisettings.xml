<settings>
    <debug>
        <extralogging>false</extralogging>
        <screenshotpath>/storage/screenshots/</screenshotpath>
    </debug>
    <general>
        <addonnotifications>true</addonnotifications>
        <settinglevel>3</settinglevel>
        <eventlog>
            <level>0</level>
            <showhigherlevels>false</showhigherlevels>
        </eventlog>
    </general>
    <locale>
        <timezone>Europe/Kiev</timezone>
        <timezonecountry>Ukraine</timezonecountry>
    </locale>
    <pvrplayback>
        <enableradiords>false</enableradiords>
        <signalquality>true</signalquality>
    </pvrplayback>
    <services>
        <airplay>false</airplay>
    </services>
    <smb>
        <legacysecurity>true</legacysecurity>
        <maxprotocol>1</maxprotocol>
        <minprotocol>1</minprotocol>
    </smb>
    <weather>
        <addon>weather.gismeteo</addon>
    </weather>
    <audio>
        <mute>false</mute>
        <fvolumelevel>0.500000</fvolumelevel>
    </audio>
</settings>
