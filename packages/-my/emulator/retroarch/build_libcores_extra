#!/bin/sh

##
## Add Amstrad CPC
CAP="$DEV_REP/libretro-cap32"
if [ ! -d $CAP ]
        then
                git clone https://github.com/libretro/libretro-cap32.git $CAP
fi
cd $CAP
git fetch
cd $CAP/linux
make clean
make
cd $old_pwd
mv $CAP/linux/*.so $ADDON_BUILD/$PKG_ADDON_ID/cores/cap32-libretro.so



