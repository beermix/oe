+class Path {
+ public:
+  Path();
+  Path(const Path& path);
+  explicit Path(const std::string& path);
+
+  // Return true of the first path item is '/'.
+  bool IsAbsolute() const;
+
+  // Return true if this is the root path (i.e. it has no parent)
+  bool IsRoot() const;
